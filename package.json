    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=developer webpack-dev-server --config ./tools/build/dev.config.ts --progress --colors",
    "build": "rm -rf ./dist && cross-env NODE_ENV=production webpack --config ./tools/build/prod.config.ts",
    "test": "cross-env NODE_ENV=test jest --config ./.jest.js --coverage --watch --colors",
    "lint": "tslint formatter:json --config ./tslint.json --exclude '**/*.d.ts' --exclude 'node_modules/**/*.ts' '**/*.ts'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{src,tools}/**/*.{ts,tsx}": [
      "prettier --parser typescript --with-node-modules --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  }
